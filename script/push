#!/bin/sh

TWD=`pwd`

TOP_DIR=`dirname $0`/..
cd "$TOP_DIR"

script/clean

cd modules
for i in core io charts mongodb pdf latex excel zoo ; do
    if [ -d "incanter-$i" -a -f "incanter-$i/project.clj" ] ; then
        OLDWD=`pwd`
        cd "incanter-$i"
        lein deploy clojars
        cd "$OLDWD"
    fi
done
cd ..

lein deploy clojars

cd "$TWD"


